    "build": "vue-tsc --emitDeclarationOnly && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "3.2.41",
    "vue-material-design-icons": "5.1.2"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "@vitejs/plugin-vue": "3.1.0",
    "autoprefixer": "10.4.10",
    "postcss": "8.4.16",
    "sass": "1.54.9",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3",
    "vite": "3.1.0",
    "vite-plugin-dts": "1.5.0",
    "vue-tsc": "1.0.9"
  }
